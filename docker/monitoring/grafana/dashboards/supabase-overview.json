{
  "dashboard": {
    "id": null,
    "title": "Supabase BaaS - Overview",
    "tags": ["supabase", "overview"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Inst칙ncias Ativas",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "none"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        },
        "targets": [
          {
            "expr": "supabase_instances_total",
            "interval": "",
            "legendFormat": "Inst칙ncias",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Targets Monitorados",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 5}
              ]
            },
            "unit": "none"
          }
        },
        "targets": [
          {
            "expr": "supabase_monitoring_targets_total",
            "legendFormat": "Targets",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Uso de CPU do Sistema",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "red", "value": 80}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU %",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "Uso de Mem칩ria do Sistema",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 85}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Mem칩ria %",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Status das Inst칙ncias",
        "type": "table",
        "gridPos": {"h": 12, "w": 24, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 0.5}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Status"},
              "properties": [
                {
                  "id": "mappings",
                  "value": [
                    {"options": {"0": {"text": "游댮 Down"}}, "type": "value"},
                    {"options": {"1": {"text": "游릭 Up"}}, "type": "value"}
                  ]
                }
              ]
            }
          ]
        },
        "targets": [
          {
            "expr": "up{job=~\"supabase-.*\"}",
            "format": "table",
            "instant": true,
            "legendFormat": "",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true,
                "job": false
              },
              "indexByName": {},
              "renameByName": {
                "Value": "Status",
                "instance_id": "Instance ID",
                "project_name": "Projeto",
                "org_id": "Org ID",
                "service": "Servi칞o",
                "port": "Porta"
              }
            }
          }
        ]
      },
      {
        "id": 6,
        "title": "Containers Docker - Status",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "none"
          }
        },
        "targets": [
          {
            "expr": "container_last_seen{name=~\".*_studio|.*_kong|.*_db\"}",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "title": "Uso de CPU por Container",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent"
          }
        },
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=~\".*_studio|.*_kong|.*_db\"}[5m]) * 100",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "refresh": "30s",
    "version": 1
  }
}